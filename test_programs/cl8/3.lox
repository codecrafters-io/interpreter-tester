class Counter {
  init(startValue) {
    if (startValue < 0) {
      print "startValue can't be negative";
      this.count = 0;
    } else {
      this.count = startValue;
    }
  }

  increment() {
    this.count = this.count + 1;
    return this.count;
  }
}

var counter1 = Counter(-<<RANDOM_INTEGER>>);
var counter2 = Counter(<<RANDOM_INTEGER>>);

print counter1.increment();
print counter2.increment();