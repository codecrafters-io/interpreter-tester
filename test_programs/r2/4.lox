var a;
var b;

fun initA() {
  a = "A refers to " + b;
}

fun initB() {
  b = "B refers to " + a;
}

a = "a";
b = "b";

print a; // expect: a
print b; // expect: b

initA();
print a; // expect: A refers to b

initB();
print b; // expect: B refers to A refers to b

a = "new a";
print b; // expect: B refers to A refers to b