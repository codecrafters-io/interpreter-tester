---
expected_error_type: none
---
class Wizard {
  castSpell(spell) {
    print "Casting a magical spell: " + spell;
  }
}

class Dragon {
  breatheFire(fire, intensity) {
    print "Breathing fire: " + fire + 
    " with intensity: " + intensity;
  }
}

// Methods on different class instances
var merlin = Wizard();
var smaug = Dragon();

// Conditional method calling
if (<<RANDOM_BOOLEAN>>) {
  merlin.castSpell("Fireball");
} else {
  smaug.breatheFire("Fire", 100);
}
