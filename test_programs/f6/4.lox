// This program creates a function that returns
// another function
// and uses it to filter a list of numbers
fun makeFilter() {
  fun filter(n) {
    if (n < <<RANDOM_INTEGER_1>>) {
      return false;
    }
    return true;
  }
  return filter;
}

// This function applies a function to a list of
// numbers
fun applyToNumbers(f, count) {
  var n = 0;
  while (n < count) {
    if (f(n)) {
      print n;
    }
    n = n + 1;
  }
}

