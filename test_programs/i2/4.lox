---
expected_error_type: none
---
class Foo {
  inFoo() {
    print "from foo";
  }
}

class Bar < Foo {
  inBar() {
    print "from bar";
  }
}

class Baz < Bar {
  inBaz() {
    print "from baz";
  }
}

// Baz should inherit the methods from both
// Foo and Bar
var baz = Baz();
baz.inFoo(); // expect: from foo
baz.inBar(); // expect: from bar
baz.inBaz(); // expect: from baz